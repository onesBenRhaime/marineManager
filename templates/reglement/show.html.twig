{% extends 'baseAdmin.html.twig' %}

{% block body %}


	<!-- Container Fluid-->
	<div class="container-fluid" id="container-wrapper">
		<div class="card">
			<div class="d-sm-flex align-items-center py-3 justify-content-between">
				<h5 class="mb-0 text-gray-800 pl-3">
					Details  Boats
				</h5>
				<ol class="breadcrumb py-0 m-0">
					<li class="breadcrumb-item">
						<a href="">Dashboard</a>
					</li>

					<li class="breadcrumb-item">
						<a href="">Boats
																																																																																																																																																																																				                    ></li>
					</li>
					<li class="breadcrumb-item">
						<a href="">Details</a>
					</li>
				</ol>
			</div>
		</div>

		<div class="row justify-content-center mt-3">
			<div
				class="col-md-10">
				<!-- Form Basic -->
				<div class="card mb-4">
					<div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
						<h6 class="m-0 font-weight-bold text-primary">
							Boats
						</h6>
					</div>

					<div class="card-body">
						<table class="table">
							<tbody>
								<tr>
									<th>Id</th>
									<td>{{reglement.id}}</td>
								</tr>
								<tr>
									<th>Num</th>
									<td>{{reglement.num}}</td>
								</tr>
								<tr>
									<th>Mat</th>
									<td>{{reglement.mat}}</td>
								</tr>
								<tr>
									<th>Date</th>
									<td>{{reglement.date?reglement.date|date('Y-m-d'):''}}</td>
								</tr>
								<tr>
									<th>Duree</th>
									<td>{{reglement.duree}}</td>
								</tr>
							</tbody>
						</table>

					</div>
				</div>
			</div>
		</div><!--Row--></div>
	<!---ContainerFluid-->
{% endblock %}
