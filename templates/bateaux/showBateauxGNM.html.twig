									</tr>
									<tr>
										<th>Port</th>
										<td>{{ bateaux.port }}</td>
									</tr>
									<tr>
										<th>Type</th>
										<td>{{ bateaux.type }}</td>
									</tr>
									<tr>
										<th>Longeur</th>
										<td>{{ bateaux.longeur }}</td>
									</tr>
									<tr>
										<th>Largeur</th>
										<td>{{ bateaux.largeur }}</td>
									</tr>
									<tr>
										<th>Hauteur</th>
										<td>{{ bateaux.hauteur }}</td>
									</tr>
									<tr>
										<th>Numserie</th>
										<td>{{ bateaux.numserie }}</td>
									</tr>
									<tr>
										<th>Marque</th>
										<td>{{ bateaux.marque }}</td>
									</tr>
									<tr>
										<th>Tonnage</th>
										<td>{{ bateaux.tonnage }}</td>
									</tr>
								</tbody>
							</table>


						</div>
					</div>
				</div>
			</div>
			<!--Row-->
		</div>
		<!---Container Fluid-->
	</div>

{% endblock %}


