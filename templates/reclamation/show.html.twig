{% extends 'baseAgentMer.html.twig' %}

{% block body %}

	<div class="page-wrapper">
		<div class="container-xl">
			<div class="page-header d-print-none">
				<div class="row align-items-center">
					<div class="col">
						<h2 class="page-title">Reclamation  Details</h2>
					</div>
				</div>
			</div>
		</div>

		<div class="page-body">
			<div class="container-xl">
				<div class="row row-cards">
					<div class="col-12">
						<div class="card mb-4">
							<div class="card-body">
								<div class="table-responsive-sm">

									<table class="table">
										<tbody>
											<tr>
												<th>Id</th>
												<td>{{ reclamation.id }}</td>
											</tr>
											<tr>
												<th>Description</th>
												<td>{{ reclamation.description }}</td>
											</tr>
											<tr>
												<th>Email Agent</th>
												<td>{{ reclamation.emailAgent }}</td>
											</tr>
										</tbody>
									</table>

								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

{% endblock %}
